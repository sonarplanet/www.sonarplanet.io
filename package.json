{
  "name": "www.sonarplanet.io",
  "version": "0.0.0",
  "description": "Landing page for Sonarplanet",
  "main": "src/main.ts",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "collectCoverage": false,
    "automock": false,
    "setupTestFrameworkScriptFile": "mock-local-storage"
  },

  
  
  "scripts": {
    "dev":
      "WEBPACK_ENV_MODE=development webpack-dev-server -p --progress --mode=development",
    "integ": "WEBPACK_ENV_MODE=integration webpack -p --progress",
    "prod": "WEBPACK_ENV_MODE=production webpack",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "mock-local-storage": "^1.0.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2",
    "html-webpack-plugin": "^3.2.0",
    "html-loader": "^0.5.5",
    "responsive-loader": "^1.1.0",
    "style-loader": "^0.21.0",
    "css-loader": "^0.28.11",
    "sass-loader": "^7.0.1",
    "ts-loader": "^4.2.0",
    "ts-jest": "^22.4.5",
    "typescript": "^2.8.3",
    "sharp": "^0.20.1",
    "string-replace-loader": "^2.1.1",
    "node-sass": "^4.8.3",
    "jest": "^22.4.3"
  },
  "repository": "git@github.com:sonarplanet/www.sonarplanet.io.git",
  "author": "Guillaume Nicolas <guillaume.nicolas@spacelephant.org>",
  "license": "Apache-2.0",
  "private": true
}
